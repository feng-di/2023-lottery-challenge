{
  "name": "2023-lottery-challenge",
  "version": "1.0.0",
  "description": "CodeCraft Challenge Lottery System",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --passWithNoTests --coverage",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "lint": "eslint --ext .js src tests",
    "start": "node src/index.js",
    "help": "node src/index.js --help",
    "buy": "node src/index.js buy",
    "draw": "node src/index.js draw",
    "winners": "node src/index.js winners"
  },
  "keywords": [
    "lottery"
  ],
  "author": "fengdi <jianfeng.xu@hotmail.com>",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ]
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.11",
    "random-firstname": "^1.0.1"
  }
}
